{
  "name": "omni-mcp-hub",
  "version": "1.0.0",
  "description": "Universal MCP server for Claude Code integration",
  "main": "dist/index.js",
  "bin": {
    "omni-mcp-hub": "./dist/index.js",
    "omni-admin": "./dist/cli/admin-ui.js",
    "omni-profile-admin": "./dist/cli/profile-admin.js",
    "omni-disclosure": "./dist/cli/mode-disclosure-cli.js",
    "omni-manual-apply": "./dist/cli/manual-apply.js",
    "omni-monitoring": "./dist/cli/monitoring-cli.js",
    "omni-config-doctor": "./dist/cli/config-doctor.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "./scripts/test-runner.sh",
    "test:watch": "vitest",
    "test:ui": "vitest --ui; ./scripts/cleanup-test-processes.sh",
    "test:coverage": "vitest run --coverage --reporter=verbose; ./scripts/cleanup-test-processes.sh",
    "test:cleanup": "./scripts/cleanup-test-processes.sh",
    "test:monitor": "./scripts/monitor-test-processes.sh",
    "test:force-stop": "pkill -f vitest || true; pkill -f 'node.*test' || true; ./scripts/cleanup-test-processes.sh",
    "pretest": "./scripts/cleanup-test-processes.sh",
    "pretest:coverage": "./scripts/cleanup-test-processes.sh",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "scan:secrets": "tsx src/cli/secrets-scan-cli.ts",
    "scan:secrets:pre-commit": "tsx src/cli/secrets-scan-cli.ts --pre-commit --fail-on high",
    "scan:secrets:ci": "tsx src/cli/secrets-scan-cli.ts --fail-on critical --output secrets-report.json",
    "admin": "tsx src/cli/admin-ui.ts",
    "admin:status": "tsx src/cli/admin-ui.ts status",
    "profile:admin": "tsx src/cli/profile-admin.ts",
    "disclosure": "tsx src/cli/mode-disclosure-cli.ts",
    "manual:apply": "tsx src/cli/manual-apply.ts",
    "monitoring": "tsx src/cli/monitoring-cli.ts",
    "config:doctor": "tsx src/cli/config-doctor.ts interactive",
    "config:check": "tsx src/cli/config-doctor.ts check",
    "config:validate": "tsx src/cli/config-doctor.ts validate",
    "install:hooks": "git config core.hooksPath .githooks",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reivosar/omni-mcp-hub.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/reivosar/omni-mcp-hub/issues"
  },
  "homepage": "https://github.com/reivosar/omni-mcp-hub#readme",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.873.0",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@types/inquirer": "^9.0.9",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "ajv": "^8.17.1",
    "async-mutex": "^0.5.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "crypto": "^1.0.1",
    "express-jwt": "^8.5.1",
    "express-rate-limit": "^8.0.1",
    "inquirer": "^12.9.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "keytar": "^7.9.0",
    "minimatch": "^9.0.3",
    "node-vault": "^0.10.5",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "winston": "^3.17.0",
    "ws": "^8.18.3",
    "yaml-schema-validator": "^1.2.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node-fetch": "^2.6.13",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "semantic-release": "^24.2.7",
    "vitest": "^3.2.4"
  }
}
